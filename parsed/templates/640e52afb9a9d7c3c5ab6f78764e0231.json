{"components/cart/shipping-estimator":"{{#if shipping_cost}}\r\n    <div class=\"cart-total-value\">\r\n        <div class=\"subtotal shipping-estimate-show\">\r\n            <a href=\"#\" class=\"shipping-estimate-value\">{{shipping_cost.formatted}}</a>\r\n        </div>\r\n    </div>\r\n{{else}}\r\n    <div class=\"cart-total-value\">\r\n        <button class=\"shipping-estimate-show\">{{lang 'cart.shipping_estimator.add_info'}}</button>\r\n        <button class=\"shipping-estimate-hide\" style=\"display: none;\">Cancel</button>\r\n    </div>\r\n{{/if}}\r\n\r\n<div class=\"shipping-estimator u-hiddenVisually\">\r\n    <form class=\"form estimator-form\" data-shipping-estimator>\r\n        <dl>\r\n            <dt class=\"estimator-form-label\">\r\n                <label class=\"form-label\" for=\"shipping-country\">{{lang 'cart.shipping_estimator.select_a_country'}}</label>\r\n            </dt>\r\n\r\n            <dd class=\"estimator-form-input\">\r\n                <select class=\"form-select\" id=\"shipping-country\" name=\"shipping-country\" data-field-type=\"Country\">\r\n                    <option>{{lang 'cart.shipping_estimator.select_a_country'}}</option>\r\n                    {{#each countries}}\r\n                        <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\r\n                            {{name}}\r\n                        </option>\r\n                    {{/each}}\r\n                </select>\r\n            </dd>\r\n\r\n            <dt class=\"estimator-form-label\">\r\n                <label class=\"form-label\" for=\"shipping-state\">{{lang 'cart.shipping_estimator.select_a_state'}}</label>\r\n            </dt>\r\n\r\n            <dd class=\"estimator-form-input\">\r\n                {{#if states.length}}\r\n                    <select class=\"form-select\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\">\r\n                        <option>{{lang 'cart.shipping_estimator.select_a_state'}}</option>\r\n                        {{#each states}}\r\n                            <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\r\n                                {{name}}\r\n                            </option>\r\n                        {{/each}}\r\n                    </select>\r\n                {{else}}\r\n                    <input class=\"form-input\" type=\"text\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\" placeholder=\"{{lang 'cart.shipping_estimator.select_a_state'}}\">\r\n                {{/if}}\r\n            </dd>\r\n\r\n            <dt class=\"estimator-form-label\">\r\n                <label class=\"form-label\" for=\"shipping-city\">{{lang 'cart.shipping_estimator.suburb_city'}}</label>\r\n            </dt>\r\n            <dd class=\"estimator-form-input\">\r\n                <input class=\"form-input\" type=\"text\" id=\"shipping-city\" name=\"shipping-city\" value=\"{{selected_city}}\" placeholder=\"{{lang 'cart.shipping_estimator.suburb_city'}}\">\r\n            </dd>\r\n\r\n            <dt class=\"estimator-form-label\">\r\n                <label class=\"form-label\" for=\"shipping-zip\">{{lang 'cart.shipping_estimator.zip_postal_code'}}</label>\r\n            </dt>\r\n            <dd class=\"estimator-form-input\">\r\n                <input class=\"form-input\" type=\"text\" id=\"shipping-zip\" name=\"shipping-zip\" value=\"{{selected_zip}}\" placeholder=\"{{lang 'cart.shipping_estimator.zip_postal_code'}}\">\r\n            </dd>\r\n            <button class=\"button button--primary button--small shipping-estimate-submit mdc-button primary\">{{lang 'cart.shipping_estimator.estimate_shipping'}}</button>\r\n        </dl>\r\n    </form>\r\n    <div class=\"shipping-quotes\"></div>\r\n</div>\r\n","components/cart/totals":"<ul class=\"cart-totals\">\r\n    <li class=\"cart-total\">\r\n        <div class=\"cart-total-label mdc-typography-roman-regular-normal \">\r\n            <strong class=\"mdc-typography-roman-regular-normal \">Discounts</strong>\r\n        </div>\r\n        <div class=\"cart-total-value mdc-typography-roman-regular-normal \">\r\n            <span class=\"checkout-totals-discounts\">Apply at checkout</span>\r\n        </div>\r\n    </li>\r\n\r\n    <li class=\"cart-total first\">\r\n        <div class=\"cart-total-label mdc-typography-roman-regular-normal \">\r\n            <strong class=\"mdc-typography-roman-regular-normal \">{{lang 'cart.checkout.subtotal'}}</strong>\r\n        </div>\r\n        <div class=\"cart-total-value mdc-typography-roman-regular-normal \">\r\n            {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n            <span>{{cart.sub_total.formatted}}</span> {{else}} {{> components/common/login-for-pricing}} {{/or}}\r\n        </div>\r\n    </li>\r\n    {{#if cart.gift_wrapping_cost.amount}}\r\n    <li class=\"cart-total mdc-typography-roman-regular-normal \">\r\n        <div class=\"cart-total-label mdc-typography-roman-regular-normal \">\r\n            <strong>{{lang 'cart.checkout.gift_wrapping'}}</strong>\r\n        </div>\r\n        <div class=\"cart-total-value mdc-typography-roman-regular-normal \">\r\n            <span>{{cart.gift_wrapping_cost.formatted}}</span>\r\n        </div>\r\n    </li>\r\n    {{/if}} {{#if cart.shipping_handling.show_estimator}}\r\n    <li class=\"cart-total mdc-typography-roman-regular-normal  checkout-page-only\">\r\n        <div class=\"cart-total-label\" mdc-typography-roman-regular-normal>\r\n            <strong>{{lang 'cart.checkout.shipping'}}</strong>\r\n        </div>\r\n        {{> components/cart/shipping-estimator cart.shipping_handling}}\r\n    </li>\r\n    <!-- {{/if}} {{#each cart.taxes}}\r\n    <li class=\"cart-total mdc-typography-roman-regular-normal \">\r\n        <div class=\"cart-total-label mdc-typography-roman-regular-normal \">\r\n            <strong>{{name}}:</strong>\r\n        </div>\r\n        <div class=\"cart-total-value mdc-typography-roman-regular-normal \">\r\n            <span>{{cost.formatted}}</span>\r\n        </div>\r\n    </li>\r\n    {{/each}} -->\r\n\r\n    {{#if cart.coupons}}\r\n        {{#each cart.coupons}}\r\n            <li class=\"cart-total coupons-data-show-checkout\">\r\n                <div class=\"cart-total-label mdc-typography-roman-regular-normal \">\r\n                    <strong>{{lang 'cart.coupons.code_label' code=code}}</strong>\r\n                    <a class=\"coupons-remove-buttons\" url=\"{{remove_url}}\">{{lang 'cart.coupons.remove'}}</a>\r\n                </div>\r\n                <div class=\"cart-total-value mdc-typography-roman-regular-normal \">\r\n                    <span>{{discount.formatted}}</span>\r\n                </div>\r\n            </li>\r\n        {{/each}}\r\n    {{/if}}    \r\n    \r\n    <li class=\"cart-total mdc-typography-roman-regular-normal checkout-page-show\">\r\n        <div class=\"cart-total-label mdc-typography-roman-regular-normal \">\r\n            <strong>Total</strong>\r\n        </div>\r\n        <div class=\"cart-total-value cart-total-grandTotal mdc-typography-roman-regular-normal\">\r\n            {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n            <span>{{cart.grand_total.formatted}}</span> {{else}} {{> components/common/login-for-pricing}} {{/or}}\r\n        </div>\r\n    </li>\r\n</ul>","components/common/alert-error":"<div class=\"alertBox alertBox--error\">\r\n    <div class=\"alertBox-column alertBox-icon\">\r\n        <icon glyph=\"ic-error\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\r\n    </div>\r\n    <p class=\"alertBox-column alertBox-message\">\r\n        <span>{{{this}}}</span>\r\n    </p>\r\n</div>\r\n","components/common/alert-modal":"<div id=\"alert-modal\" class=\"modal modal--alert modal--small\" data-reveal data-prevent-quick-search-close>\r\n    <div class=\"swal2-icon swal2-error swal2-animate-error-icon\"><span class=\"swal2-x-mark swal2-animate-x-mark\"><span class=\"swal2-x-mark-line-left\"></span><span class=\"swal2-x-mark-line-right\"></span></span></div>\r\n\r\n    <div class=\"modal-content\"></div>\r\n\r\n    <div class=\"button-container\"><button type=\"button\" class=\"confirm button\" data-reveal-close>OK</button></div>\r\n</div>\r\n","components/common/body":"<div class=\"body\" data-currency-code=\"{{currency_selector.active_currency_code}}\">\r\n    {{#block \"hero\"}} {{/block}}\r\n    <div class=\"container\">\r\n        {{#block \"page\"}} {{/block}}\r\n    </div>\r\n    {{> components/common/modal}}\r\n    {{> components/common/alert-modal}}\r\n</div>\r\n","components/common/breadcrumbs":"<ul class=\"breadcrumbs\" itemscope itemtype=\"http://schema.org/BreadcrumbList\">\r\n    {{#unless theme_settings.hide_breadcrumbs }}\r\n        {{#each breadcrumbs}}\r\n            <li class=\"breadcrumb {{#if @last}}is-active{{/if}}\" itemprop=\"itemListElement\" itemscope itemtype=\"http://schema.org/ListItem\">\r\n                {{#or @last (if url \"==\" null)}}\r\n                    <meta itemprop=\"item\" content=\"{{url}}\">\r\n                    <span class=\"breadcrumb-label\" itemprop=\"name\">{{name}}</span>\r\n                {{else}}\r\n                    <a href=\"{{url}}\" class=\"breadcrumb-label\" itemprop=\"item\"><span itemprop=\"name\">{{name}}</span></a>\r\n                {{/or}}\r\n                <meta itemprop=\"position\" content=\"{{add @index 1}}\" />\r\n            </li>\r\n        {{/each}}\r\n    {{/unless}}\r\n</ul>\r\n","components/common/cookie":"<div class=\"cookieMessage\">\r\n    <div class=\"cookieMessage-container\">\r\n        <div class=\"cookieMessage-text\">\r\n            <p>{{settings.privacy_cookie}}</p>\r\n        </div>\r\n        <button class=\"button button--primary button--small\" data-privacy-accept>{{lang 'cookies.accept'}}</button>\r\n    </div>\r\n</div>\r\n","components/common/footer":"{{#if template '==' 'pages/home'}}\r\n\r\n {{else}}\r\n<footer class=\"wc-footer-custom\">\r\n    <div class=\"wc-footer__menu-custom\">\r\n        <a class=\"wc-footer__menu-custom-link\" href=\"https://support.wildcrafter.com/hc/en-us/categories/360002006492-Returns\">Refund Policy</a>\r\n    </div>\r\n    <div class=\"wc-footer__menu-custom\">\r\n        <a class=\"wc-footer__menu-custom-link\" href=\"https://wildcrafter.com/privacy-policy/\">Privacy Policy</a>\r\n    </div>\r\n    <div class=\"wc-footer__menu-custom\">\r\n        <a class=\"wc-footer__menu-custom-link\" href=\"https://wildcrafter.com/terms-of-service/\">Terms of service</a>\r\n    </div>\r\n</footer>\r\n{{/if}}","components/common/header":"{{#if banners.top}}\r\n<div class=\"banners\" data-banner-location=\"top\">\r\n   {{#each (limit banners.top_metadata 1)}}\r\n   <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-banner-id=\"{{this.banner-name}}\">\r\n       <div data-event-type=\"promotion-click\">\r\n           {{{this.content}}}\r\n       </div>\r\n   </div>\r\n   {{/each}}\r\n</div>\r\n{{/if}}\r\n<aside class=\"wc-drawer-left mdc-drawer mdc-drawer--modal\">\r\n    <div class=\"wc-drawer-left__header mdc-drawer__header\">\r\n        <div class=\"wc-drawer-left__header-menu\">\r\n            <div class=\"wc-drawer-left__header-menu-item\" data-menu-item-id=\"185\">Shop</div>\r\n            <div class=\"wc-drawer-left__header-menu-item\" data-menu-item-id=\"186\">Learn</div>\r\n        </div>\r\n        <div class=\"wc-drawer-left__flex-1\"></div>\r\n        <a href=\"\" title=\"Close\" class=\"wc-drawer-left__close mdc-drawer__close\" id=\"js-drawer-close-button\">\r\n            <i class=\"icon-arrow-left\"></i>\r\n        </a>\r\n    </div>\r\n    <div class=\"wc-drawer-left__content\">\r\n        <div class=\"wc-drawer-left__menu-children\">\r\n            <div class=\"wc-drawer-left__menu-children-pane\" data-menu-item-id=\"185\">                    \r\n                {{#each theme_settings.homepage_left_menu_titles.menus.Shop}}\r\n                <div class=\"wc-drawer-left__menu-children-pane-item {{this.color}}\">\r\n                    <a class=\"wc-drawer-left__menu-children-pane-item-link\" href=\"{{this.url}}\">\r\n                        {{this.label}}\r\n                    </a>\r\n                </div>\r\n                {{/each}}\r\n            </div>\r\n            <div class=\"wc-drawer-left__menu-children-pane\" data-menu-item-id=\"186\">\r\n                {{#each theme_settings.homepage_left_menu_titles.menus.Learn}}\r\n                <div class=\"wc-drawer-left__menu-children-pane-item {{this.color}}\">\r\n                    <a class=\"wc-drawer-left__menu-children-pane-item-link\" href=\"{{this.url}}\">\r\n                        {{this.label}}\r\n                        {{#if this.smal_label}}\r\n                            <span>{{this.smal_label}}</span>\r\n                        {{/if}}\r\n                    </a>\r\n                </div>\r\n                {{/each}}\r\n            </div>\r\n        </div>\r\n\r\n        <div class=\"wc-drawer-left__separator-line\"></div>\r\n\r\n        <div class=\"wc-drawer-left__second-menu\">\r\n            {{#each theme_settings.homepage_left_menu_titles.pages}}\r\n            <a class=\"wc-drawer-left__second-menu-item\" href=\"{{this.url}}\">\r\n                {{this.label}}\r\n                {{#if this.smal_label}}\r\n                    <span>{{this.smal_label}}</span>\r\n                {{/if}}\r\n            </a>\r\n            {{/each}}\r\n        </div>\r\n\r\n        <div class=\"wc-drawer-left__flex-1\"></div>\r\n\r\n        <div class=\"wc-drawer-left__social-icons\">\r\n            {{#each theme_settings.homepage_left_menu_titles.social}}\r\n            <a class=\"wc-drawer-left__social-icon\" href=\"{{this.url}}\">\r\n                <i class=\"{{this.icon}}\"></i>\r\n            </a>\r\n            {{/each}}\r\n        </div>\r\n    </div>\r\n</aside>\r\n\r\n<div class=\"mdc-drawer-scrim\"></div>\r\n<aside class=\"wc-drawer-right mdc-drawer mdc-drawer--modal\" dir=\"rtl\">\r\n    <div class=\"wc-drawer-right__header mdc-drawer__header\" dir=\"ltr\">\r\n        <div class=\"wc-drawer-right__header-top\">\r\n            <a href=\"\" title=\"Close\" class=\"wc-drawer-right__header-close mdc-drawer__close\" id=\"js-drawer-right-close-button\">\r\n                <i class=\"icon-arrow-right\"></i>\r\n            </a>\r\n            <div class=\"wc-drawer-right__header-title\">YOUR CART</div>\r\n        </div>\r\n    </div>    \r\n    <div class=\"wc-drawer-right__content\" dir=\"ltr\" height=\"100%\">\r\n        <div class=\"page custom-cart-menu\">\r\n            <main class=\"page-content\" data-cart>  \r\n                    <div class=\"loadingOverlay\"></div>                           \r\n               <div class=\"data-cart-show-menu\" data-cart-show-menu>\r\n                    \r\n                    <div class=\"cart-delivery-content\">\r\n                        <span class=\"cart-delivery-icons\"><i class=\"wc-drawer-right__header-icon icon-delivery\"></i></span>\r\n                        <span class=\"cart-delivery-text\">Almost there! Free US shipping on orders of 2 packs or more.</span>\r\n                    </div>\r\n                    <div data-cart-totals>\r\n                        {{> components/cart/totals}}\r\n                    </div>\r\n                    <div class=\"checkout-btn-view\">\r\n                        <a href=\"{{urls.checkout.single_address}}\" class=\"mdc-button mdc-button--primary\" value=\"{{lang 'cart.checkout.title' }}\"><span>checkout</span></a>\r\n                    </div>\r\n                </div>\r\n                <!-- <section class=\"bc-cart\" empty-cart-view-menu>\r\n                        <img class=\"shopping-cart__fill-your-cup-img\" src=\"{{cdn 'images/fill_your_cup.svg'}}\" alt=\"\">\r\n                        <div class=\"shopping-cart shopping-cart--empty\">\r\n                            <img class=\"shopping-cart__empty-cup-img\" src=\"{{cdn 'images/empty_cup.png'}}\" alt=\"\">\r\n                        </div>\r\n                        <a class=\"shopping-cart__continue-shopping-button mdc-button primary\" href=\"../\">continue shopping</a>\r\n                </section> -->\r\n                <section class=\"bc-cart\" empty-cart-view-menu>\r\n                        <div class=\"shopping-cart shopping-cart--empty\" id=\"js-shopping-cart-root\" data-cart-id=\"\">\r\n                          <img class=\"shopping-cart__fill-your-cup-img\" src=\"{{cdn 'images/fill_your_cup.svg'}}\" alt=\"\">\r\n                          <img class=\"shopping-cart__empty-cup-img\" src=\"{{cdn 'images/empty_cup.png'}}\" alt=\"\">\r\n                          <a class=\"shopping-cart__continue-shopping-button mdc-button primary\" href=\"\">continue shopping</a>\r\n                        </div>\r\n                </section>                  \r\n            </main>\r\n        </div>\r\n    </div>\r\n</aside>\r\n\r\n<div class=\"mdc-drawer-scrim\"></div>\r\n\r\n<div style=\"position: absolute; z-index: -999999; opacity: 0;\">\r\n    <i class=\"md-icons\">chevron_right</i>\r\n</div>\r\n<header class=\"wc-header\" id=\"js-sticky-header\">\r\n    <div class=\"wc-header__container\">        \r\n        <div class=\"wc-header__left-menu\">\r\n            <div class=\"header__continue_shopping\">\r\n                <a href=\"http://wildcrafter.com/\"> \r\n                    <i class=\"md-icons\">arrow_back</i>\r\n                    <span>CONTINUE SHOPPING </span>\r\n                </a>\r\n            </div>\r\n        </div>\r\n        <div class=\"wc-header__logo-wrapper\">\r\n            <a class=\"wc-header__logo-container\" href=\"../\">\r\n                <!-- <img class=\"wc-header__logo-large\" src=\"{{cdn 'tiles/wildcrafter-logo.svg'}}\"></img> -->\r\n                <img class=\"wc-header__logo-large\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\"></img>\r\n                <i class=\"wc-header__logo-sticky icon-wildcrafter-logo\"></i>\r\n                \r\n            </a>\r\n            {{#if template '==' 'pages/home'}}\r\n            <style>\r\n                .wc-header__logo-wrapper{top:220%}\r\n                .loading {font-size: 20px;text-align: left;margin-left: 20px; margin-top:10px;cursor: pointer}                  \r\n                .loading:after {overflow: hidden;display: inline-block;vertical-align: bottom;-webkit-animation: ellipsis steps(4,end) 900ms infinite; animation: ellipsis steps(4,end) 900ms infinite;content: \"\\2026\"; width: 0px; } \r\n                                  \r\n                @keyframes ellipsis {\r\n                  to {\r\n                    width: 1.25em;    \r\n                  }\r\n                }\r\n\r\n                @-webkit-keyframes ellipsis {\r\n                  to {\r\n                    width: 1.25em;    \r\n                  }\r\n                }\r\n            </style>\r\n           <div class=\"loading\" onclick=\"window.location.href='http://wildcrafter.com/'\">Redirecting to Shop</div>\r\n            {{/if}}\r\n        </div>\r\n    </div>\r\n</header>\r\n\r\n<div class=\"wc-header-placeholder\"></div>\r\n","components/common/login-for-pricing":"<p translate>Log in for pricing</p>","components/common/modal":"<div id=\"modal\" class=\"modal\" data-reveal data-prevent-quick-search-close>\r\n    <a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" role=\"button\">\r\n        <span aria-hidden=\"true\">&#215;</span>\r\n    </a>\r\n    <div class=\"modal-content\"></div>\r\n    <div class=\"loadingOverlay\"></div>\r\n</div>\r\n","components/gift-certificate/navigation":"<nav class=\"navBar navBar--sub\">\r\n    <ul class=\"navBar-section account-navigation\">\r\n        {{#if gift_page '===' 'purchase'}}\r\n            <li class=\"navBar-item is-active\">\r\n                {{lang 'gift_certificate.purchase.heading'}}\r\n            </li>\r\n        {{else}}\r\n            <li class=\"navBar-item\">\r\n                <a class=\"navBar-action\" href=\"{{urls.gift_certificate.purchase}}\">\r\n                    {{lang 'gift_certificate.purchase.heading'}}\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if gift_page '===' 'redeem'}}\r\n            <li class=\"navBar-item is-active\">\r\n                {{lang 'gift_certificate.redeem.heading'}}\r\n            </li>\r\n        {{else}}\r\n            <li class=\"navBar-item\">\r\n                <a class=\"navBar-action\" href=\"{{urls.gift_certificate.redeem}}\">\r\n                    {{lang 'gift_certificate.redeem.heading'}}\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if gift_page '===' 'balance'}}\r\n            <li class=\"navBar-item is-active\">\r\n                {{lang 'gift_certificate.balance.heading'}}\r\n            </li>\r\n        {{else}}\r\n            <li class=\"navBar-item\">\r\n                <a class=\"navBar-action\" href=\"{{urls.gift_certificate.balance}}\">\r\n                    {{lang 'gift_certificate.balance.heading'}}\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n    </ul>\r\n</nav>\r\n","layout/base":"<!DOCTYPE html>\r\n<html class=\"no-js\" lang=\"{{ locale_name }}\">\r\n    <head>\r\n        <title>{{ head.title }}</title>\r\n        {{{ resourceHints }}}\r\n        {{{ head.meta_tags }}}\r\n        {{{ head.config }}}\r\n        {{#block \"head\"}} {{/block}}\r\n\r\n        <link href=\"{{ head.favicon }}\" rel=\"shortcut icon\">\r\n        <link rel=\"stylesheet\" href=\"https://fonts.googleapis.com/icon?family=Material+Icons\">\r\n        <meta name=\"viewport\" content=\"width=device-width, initial-scale=1, maximum-scale=1\">\r\n        <!-- <meta http-equiv=\"refresh\" content=\"0;URL=/test.php\"> -->\r\n        <script src=\"{{cdn 'assets/js/custom/jquery-ver=1.12.4.js'}}\"></script>\r\n\r\n        <script>\r\n            {{!-- Change document class from no-js to js so we can detect this in css --}}\r\n            document.documentElement.className = document.documentElement.className.replace('no-js', 'js');\r\n        </script>\r\n        \r\n        <script>\r\n        window.addEventListener('load', function() {\r\n            if (top == self) {\r\n                document.querySelector('.wc-header').style.display = 'block';\r\n                document.querySelector('.wc-footer-custom').style.display = 'block';\r\n                jQuery(\"div\").removeClass('framework');\r\n                if(jQuery(\"div\").hasClass(\"fixed_area\"))\r\n                    document.querySelector('.fixed_area').style.position = 'relative';\r\n                //jQuery(\".container\").css(\"padding\", \"0px\");\r\n            }\r\n        })\r\n\r\n        </script>\r\n\r\n        {{ getFontsCollection }}\r\n        {{{stylesheet '/assets/css/theme.css'}}}\r\n\r\n        {{{head.scripts}}}\r\n        {{{head.rsslinks}}}\r\n\r\n        {{~inject 'themeSettings' theme_settings}}\r\n        {{~inject 'genericError' (lang 'common.generic_error')}}\r\n        {{~inject 'maintenanceMode' settings.maintenance}}\r\n        {{~inject 'urls' urls}}\r\n        {{~inject 'secureBaseUrl' settings.secure_base_url}}\r\n        {{~inject 'cartId' cart_id}}\r\n        {{~inject 'template' template}}\r\n    </head>\r\n    <body>\r\n        <svg data-src=\"{{cdn 'img/icon-sprite.svg'}}\" class=\"icons-svg-sprite\"></svg>\r\n\r\n        {{~#and settings.privacy_cookie settings.is_eu_ip_address}}\r\n            {{> components/common/cookie}}\r\n        {{/and}}\r\n\r\n        {{> components/common/header }}\r\n        {{> components/common/body }}\r\n        {{> components/common/footer }}\r\n\r\n        <script>window.__webpack_public_path__ = \"{{cdn 'assets/dist/'}}\";</script>\r\n        <script src=\"{{cdn 'assets/dist/theme-bundle.main.js'}}\"></script>\r\n        <script>\r\n            {{!-- Exported in app.js --}}\r\n            window.stencilBootstrap(\"{{page_type}}\", {{jsContext}}).load();\r\n        </script>\r\n\r\n        {{{footer.scripts}}}\r\n        <script src=\"{{cdn 'assets/js/custom/main.min.js'}}\"></script>\r\n        <script src=\"{{cdn 'assets/js/custom/vendor.min.js'}}\"></script>\r\n    </body>\r\n</html>\r\n","pages/gift-certificate/balance":"{{#partial \"page\"}}\r\n\r\n{{> components/common/breadcrumbs breadcrumbs=breadcrumbs}}\r\n<h1 class=\"page-heading\">{{lang 'gift_certificate.heading' }}</h1>\r\n{{> components/gift-certificate/navigation gift_page='balance'}}\r\n\r\n<main class=\"account account--fixed\">\r\n    <h3>{{lang 'gift_certificate.balance.heading'}}</h3>\r\n    <p>\r\n        <strong>{{lang 'gift_certificate.balance.intro' }}</strong>\r\n    </p>\r\n\r\n    {{#if gift_certificate}}\r\n        {{#if gift_certificate.error}}\r\n            {{> components/common/alert-error gift_certificate.error}}\r\n        {{else}}\r\n            <h1>{{gift_certificate.balance.formatted}}</h1>\r\n            <h3>{{gift_certificate.code}}</h3>\r\n        {{/if}}\r\n    {{/if}}\r\n\r\n    <form class=\"form form--oneField\" id=\"gift-certificate-balance\" method=\"get\">\r\n        <input type=\"hidden\" name=\"action\" value=\"balance\">\r\n        <div class=\"form-field\">\r\n            <label class=\"form-label\" for=\"giftcertificatecode\">\r\n                {{lang 'forms.gift_certificate.balance.code' }}\r\n            </label>\r\n            <div class=\"form-prefixPostfix wrap\">\r\n                <input class=\"form-input\" id=\"giftcertificatecode\" name=\"giftcertificatecode\">\r\n                <input type=\"submit\" class=\"button button--primary form-prefixPostfix-button--postfix\" value=\"{{lang 'forms.gift_certificate.balance.submit_value' }}\">\r\n            </div>\r\n        </div>\r\n    </form>\r\n</main>\r\n{{/partial}}\r\n{{> layout/base}}\r\n"}
