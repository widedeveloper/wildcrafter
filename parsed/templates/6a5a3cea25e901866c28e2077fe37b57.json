{"components/common/forms/text":"<div class=\"form-field \">\r\n    <div class=\"mdc-text-field \" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\r\n        <label class=\"form-label mdc-floating-label\" for=\"{{id}}_input\">{{label}}\r\n        </label>\r\n        <input type=\"text\" id=\"{{id}}_input\" data-label=\"{{label}}\" name=\"{{name}}\" {{#if value}} value=\"{{value}}\"{{/if}} {{#if placeholder}} \r\n        placeholder=\"{{placeholder}}\"{{/if}} class=\"form-input mdc-text-field__input\" aria-required=\"{{required}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n        \r\n    </div>\r\n\r\n</div>\r\n\r\n<!-- \r\n<div class=\"mdc-ripple-upgraded mdc-text-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\r\n    <label class=\"mdc-floating-label\" for=\"{{id}}_input\">{{label}}        \r\n    </label>\r\n    <input type=\"text\" id=\"{{id}}_input\" data-label=\"{{label}}\" name=\"{{name}}\" {{#if value}} value=\"{{value}}\"{{/if}} class=\"mdc-text-field__input\" aria-required=\"{{required}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n    {{#if required}}<div class=\"mdc-line-ripple\"></div>{{/if}}\r\n</div> -->\r\n\r\n\r\n"}
