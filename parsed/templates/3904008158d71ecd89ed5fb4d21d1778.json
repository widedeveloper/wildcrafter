{"components/cart/content":"<div class=\"shopping-cart__products shopping-cart_page_products\" data-cart-quantity=\"{{cart.quantity}}\">\r\n    {{#each cart.items}}\r\n    <div class=\"shopping-cart__product cart-page-products\" data-item-row data-card-productid={{product_id}}>\r\n        <a target =\"_top\" href=\"{{#each custom_fields}}{{#if name '==' 'wp_url'}}{{{value}}}{{/if}}{{/each}}\">\r\n        <div class=\"shopping-cart__product-img-container\" >\r\n            {{#if type '==' 'GiftCertificate'}}\r\n            <img class=\"shopping-cart__product-img lazyload\" data-sizes=\"auto\" src=\"{{cdn ../../diveme_settings.default_image_gift_certificate}}\"\r\n                alt=\"GiftCertificate\" title=\"GiftCertificate\"> {{else}}\r\n            <img class=\"shopping-cart__product-img  lazyload\" data-sizes=\"auto\" src=\"{{getImage image 'productthumb_size' (cdn ../../diveme_settings.default_image_product)}}\"\r\n                alt=\"{{image.alt}}\" title=\"{{image.alt}}\"> {{/if}}\r\n        </div>\r\n        </a>\r\n        <div class=\"shopping-cart__product-info-container\">\r\n                <a target =\"_top\" href=\"{{#each custom_fields}}{{#if name '==' 'wp_url'}}{{{value}}}{{/if}}{{/each}}\"><div class=\"shopping-cart__product-info-label\">{{name}}</div></a>\r\n                {{#each custom_fields}}          \r\n                    {{#if name '==' 'Sub Title'}}\r\n                    <div class=\"shopping-cart__product-info-sub\" >{{{value}}}</div>\r\n                    {{/if}}\r\n                {{/each}}\r\n            <div class=\"shopping-cart__hline\"></div>\r\n            <div class=\"shopping-cart__product-info-3part\">\r\n                {{price.formatted}}\r\n                {{#each custom_fields}}   \r\n                    {{#if name '==' 'boxes'}}\r\n                    | {{{value}}} \r\n                    {{/if}}                        \r\n                {{/each}}\r\n                {{#each custom_fields}}\r\n                {{#if name '==' 'cups'}}\r\n                    | {{{value}}}\r\n                    {{/if}}\r\n                {{/each}}\r\n            </div>\r\n            <div class=\"shopping-cart__hline\"></div>\r\n            <div class=\"form-increment\">\r\n\r\n                    {{# if can_modify}}\r\n                    <button class=\"button button--icon\" data-cart-update data-cart-itemid=\"{{id}}\" data-action=\"dec\">\r\n                            <i class=\"material-icons mdc-text-field__icon icon-remove-left\">remove</i>\r\n                    </button>\r\n                    {{/if}}\r\n                    <input class=\"form-input form-input--incrementTotal cart-item-qty-input\" id=\"qty-{{id}}\" name=\"qty-{{id}}\" type=\"tel\" value=\"{{quantity}}\"\r\n                        data-quantity-min=\"{{min_purchase_quantity}}\" data-quantity-max=\"{{max_purchase_quantity}}\" data-quantity-min-error=\"{{lang 'products.quantity_min' quantity=min_purchase_quantity}}\"\r\n                        data-quantity-max-error=\"{{lang 'products.quantity_max' quantity=max_purchase_quantity}}\" min=\"1\" pattern=\"[0-9]*\"\r\n                        data-cart-itemid=\"{{id}}\" data-action=\"manualQtyChange\" aria-live=\"polite\" {{#unless can_modify}} disabled{{/unless}}>\r\n                    {{# if can_modify}}\r\n                    <button class=\"button button--icon\" data-cart-update data-cart-itemid=\"{{id}}\" data-action=\"inc\">\r\n                            <i class=\"material-icons mdc-text-field__icon icon-add-right\">add</i>\r\n                    </button>\r\n                    {{/if}}\r\n                <!-- </div> -->\r\n\r\n                <!-- <span class=\"cart-item-label\">{{lang 'cart.checkout.total'}}</span> -->\r\n                {{#or ../customer (if ../diveme_settings.restrict_to_login '!==' true)}}\r\n                <!-- <strong class=\"cart-item-value all-total-value mdc-typography-roman-regular-normal {{#if total_discounted}}price--discounted{{/if}}\">{{total.formatted}}</strong> -->\r\n                {{#if total_discounted}}\r\n                <!-- <strong class=\"cart-item-value all-total-discounted mdc-typography-roman-regular-normal\">{{total_discounted.formatted}}</strong> -->\r\n                {{/if}} \r\n                \r\n                {{else}} {{> components/common/login-for-pricing}} {{/or}} {{# if can_modify}}\r\n                <!-- <a class=\"cart-remove icon\" data-cart-itemid=\"{{id}}\" href=\"#\" data-confirm-delete=\"{{lang 'cart.confirm_delete'}}\">\r\n                    <svg>\r\n                        <use xlink:href=\"#icon-close\"></use>\r\n                    </svg>\r\n                </a> -->\r\n                {{else}} {{#if type '==' 'GiftCertificate'}}\r\n                <!-- <a class=\"cart-remove icon\" data-cart-itemid=\"{{id}}\" href=\"#\" data-confirm-delete=\"{{lang 'cart.confirm_delete'}}\">\r\n                    <svg>\r\n                        <use xlink:href=\"#icon-close\"></use>\r\n                    </svg>\r\n                </a> -->\r\n                {{/if}} {{/if}}\r\n\r\n            </div>\r\n        </div>\r\n                {{#or ../customer (if ../theme_settings.restrict_to_login '!==' true)}}\r\n                    <div class=\"shopping-cart__product_price  {{#if total_discounted}}price--discounted{{/if}}\">{{total.formatted}}</div>\r\n                    {{#if total_discounted}}\r\n                        <div class=\"shopping-cart__product_price\">{{total_discounted.formatted}}</div>\r\n                    {{/if}}\r\n                {{/or}}\r\n                <a class=\"cart-remove shopping-cart__product-close-icon icon\" data-cart-itemid=\"{{id}}\" href=\"#\" data-confirm-delete=\"{{lang 'cart.confirm_delete'}}\">\r\n                    <i class=\"material-icons mdc-text-field__icon icon-remove-right\">highlight_off</i>\r\n                </a>\r\n\r\n    </div>\r\n    {{/each}}\r\n       \r\n</div>","components/common/login-for-pricing":"<p translate>Log in for pricing</p>"}
