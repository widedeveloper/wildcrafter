{"components/account/edit-account":"<form action=\"{{urls.account.update_action}}\" data-edit-account-form class=\"form\" method=\"post\">\r\n    {{inject 'firstName' (lang 'forms.validate.account.edit.first_name')}}\r\n    {{inject 'lastName' (lang 'forms.validate.account.edit.last_name')}}\r\n    {{inject 'phoneNumber' (lang 'forms.validate.account.edit.phone')}}\r\n    {{inject 'currentPassword' (lang 'forms.validate.account.edit.password')}}\r\n    <fieldset class=\"form-fieldset\">\r\n        <div class=\"form-row form-row--half\">\r\n            <div class=\"form-field\">\r\n                <div class=\"mdc-text-field\">\r\n                    <label class=\"form-label mdc-floating-label\">{{lang 'account.settings.first_name' }}</label>                \r\n                    <input type=\"text\" class=\"form-input  mdc-text-field__input\" name=\"account_firstname\" id=\"account_firstname\" value=\"{{forms.edit_account.first_name}}\">\r\n                </div> \r\n            </div>\r\n            <div class=\"form-field\">\r\n                    <div class=\"mdc-text-field\">\r\n                            <label class=\"form-label mdc-floating-label\">{{lang 'account.settings.last_name' }}</label>                \r\n                            <input type=\"text\" class=\"form-input  mdc-text-field__input\" name=\"account_lastname\" id=\"account_lastname\" value=\"{{forms.edit_account.last_name}}\">\r\n                    </div>\r\n            </div>\r\n            <div class=\"form-field\">\r\n                    <div class=\"mdc-text-field\">\r\n                            <label class=\"form-label mdc-floating-label\">{{lang 'account.settings.company' }}</label>                \r\n                            <input type=\"text\" class=\"form-input  mdc-text-field__input\" name=\"account_companyname\" id=\"account_companyname\" value=\"{{forms.edit_account.company_name}}\">\r\n                    </div>\r\n            </div>\r\n            <div class=\"form-field\">\r\n                    <div class=\"mdc-text-field\">\r\n                            <label class=\"form-label mdc-floating-label\">{{lang 'account.settings.phone' }}</label>                \r\n                            <input type=\"text\" class=\"form-input  mdc-text-field__input\" name=\"account_phone\" id=\"account_phone\" value=\"{{forms.edit_account.phone}}\">\r\n                    </div>\r\n            </div>\r\n            {{#each forms.edit_account.fields}}\r\n                {{{dynamicComponent 'components/common/forms'}}}\r\n            {{/each}}\r\n        </div>\r\n    </fieldset>\r\n    <div class=\"form-actions\">\r\n        <button class=\"button mdc-button mdc-button--x-small primary\">{{lang 'account.settings.update_details'}}</button>\r\n    </div>\r\n</form>\r\n","components/account/navigation":"<nav class=\"navBar navBar--sub navBar--account\">\r\n    <ul class=\"navBar-section\">\r\n        <li class=\"navBar-item\">\r\n                <a class=\"navBar-action\" href=\"../checkout\" target=\"_self\">Checkout</a>\r\n        </li>\r\n        {{#if account_page '===' 'orders'}}\r\n            {{#if page_type '===' 'account_order'}}\r\n                <li class=\"navBar-item is-active\">\r\n                    <a class=\"navBar-action\" href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a>\r\n                </li>\r\n            {{else}}\r\n                <li class=\"navBar-item is-active\">{{lang 'account.nav.orders'}}</li>\r\n            {{/if}}\r\n        {{else}}\r\n            <li class=\"navBar-item\">\r\n                <a class=\"navBar-action\" href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if settings.returns_enabled}}\r\n            {{#if account_page '===' 'returns'}}\r\n                <li class=\"navBar-item is-active\">{{lang 'account.nav.returns'}}</li>\r\n            {{else}}\r\n                <li class=\"navBar-item\">\r\n                    <a class=\"navBar-action\" href=\"{{urls.account.returns}}\">{{lang 'account.nav.returns'}}</a>\r\n                </li>\r\n            {{/if}}\r\n        {{/if}}\r\n        {{#if account_page '===' 'messages'}}\r\n            <li class=\"navBar-item is-active\">{{lang 'account.nav.messages' num_new_messages=customer.num_new_messages}}</li>\r\n        {{else}}\r\n            <li class=\"navBar-item\">\r\n                <a class=\"navBar-action\" href=\"{{urls.account.inbox}}\">{{lang 'account.nav.messages' num_new_messages=customer.num_new_messages}}</a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if account_page '===' 'addresses'}}\r\n            <li class=\"navBar-item is-active\">{{lang 'account.nav.addresses'}}</li>\r\n        {{else}}\r\n            <li class=\"navBar-item\">\r\n                <a class=\"navBar-action\" href=\"{{urls.account.addresses}}\">{{lang 'account.nav.addresses'}}</a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if settings.show_payment_methods}}\r\n            {{#if account_page '===' 'payment_methods'}}\r\n                <li class=\"navBar-item is-active\">{{lang 'account.nav.payment_methods'}}</li>\r\n            {{else}}\r\n                <li class=\"navBar-item\">\r\n                    <a class=\"navBar-action\" href=\"{{urls.account.payment_methods.all}}\">{{lang 'account.nav.payment_methods'}}</a>\r\n                </li>\r\n            {{/if}}\r\n        {{/if}}\r\n        <!-- {{#if settings.show_wishlist}}\r\n            {{#if account_page '===' 'wishlists'}}\r\n                <li class=\"navBar-item is-active\">{{lang 'account.nav.wishlists' num_wishlists=customer.num_wishlists}}</li>\r\n            {{else}}\r\n                <li class=\"navBar-item\">\r\n                    <a class=\"navBar-action\" href=\"{{urls.account.wishlists.all}}\">{{lang 'account.nav.wishlists' num_wishlists=customer.num_wishlists}}</a>\r\n                </li>\r\n            {{/if}}\r\n        {{/if}} -->\r\n        <!-- {{#if account_page '===' 'recent_items'}}\r\n            <li class=\"navBar-item is-active\">{{lang 'account.nav.recently_viewed'}}</li>\r\n        {{else}}\r\n            <li class=\"navBar-item\">\r\n                <a class=\"navBar-action\" href=\"{{urls.account.recent_items}}\">{{lang 'account.nav.recently_viewed'}}</a>\r\n            </li>\r\n        {{/if}} -->\r\n        {{#if account_page '===' 'settings'}}\r\n            <li class=\"navBar-item is-active\">{{lang 'account.nav.settings'}}</li>\r\n        {{else}}\r\n            <li class=\"navBar-item\">\r\n                <a class=\"navBar-action\" href=\"{{urls.account.details}}\">{{lang 'account.nav.settings'}}</a>\r\n            </li>\r\n        {{/if}}\r\n        <li class=\"navBar-item\">\r\n            <a class=\"navBar-action\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\r\n        </li>\r\n    </ul>\r\n</nav>\r\n","components/cart/shipping-estimator":"{{#if shipping_cost}}\r\n    <div class=\"cart-total-value\">\r\n        <div class=\"subtotal shipping-estimate-show\">\r\n            <a href=\"#\" class=\"shipping-estimate-value\">{{shipping_cost.formatted}}</a>\r\n        </div>\r\n    </div>\r\n{{else}}\r\n    <div class=\"cart-total-value\">\r\n        <button class=\"shipping-estimate-show\">{{lang 'cart.shipping_estimator.add_info'}}</button>\r\n        <button class=\"shipping-estimate-hide\" style=\"display: none;\">Cancel</button>\r\n    </div>\r\n{{/if}}\r\n\r\n<div class=\"shipping-estimator u-hiddenVisually\">\r\n    <form class=\"form estimator-form\" data-shipping-estimator>\r\n        <dl>\r\n            <dt class=\"estimator-form-label\">\r\n                <label class=\"form-label\" for=\"shipping-country\">{{lang 'cart.shipping_estimator.select_a_country'}}</label>\r\n            </dt>\r\n\r\n            <dd class=\"estimator-form-input\">\r\n                <select class=\"form-select\" id=\"shipping-country\" name=\"shipping-country\" data-field-type=\"Country\">\r\n                    <option>{{lang 'cart.shipping_estimator.select_a_country'}}</option>\r\n                    {{#each countries}}\r\n                        <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\r\n                            {{name}}\r\n                        </option>\r\n                    {{/each}}\r\n                </select>\r\n            </dd>\r\n\r\n            <dt class=\"estimator-form-label\">\r\n                <label class=\"form-label\" for=\"shipping-state\">{{lang 'cart.shipping_estimator.select_a_state'}}</label>\r\n            </dt>\r\n\r\n            <dd class=\"estimator-form-input\">\r\n                {{#if states.length}}\r\n                    <select class=\"form-select\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\">\r\n                        <option>{{lang 'cart.shipping_estimator.select_a_state'}}</option>\r\n                        {{#each states}}\r\n                            <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\r\n                                {{name}}\r\n                            </option>\r\n                        {{/each}}\r\n                    </select>\r\n                {{else}}\r\n                    <input class=\"form-input\" type=\"text\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\" placeholder=\"{{lang 'cart.shipping_estimator.select_a_state'}}\">\r\n                {{/if}}\r\n            </dd>\r\n\r\n            <dt class=\"estimator-form-label\">\r\n                <label class=\"form-label\" for=\"shipping-city\">{{lang 'cart.shipping_estimator.suburb_city'}}</label>\r\n            </dt>\r\n            <dd class=\"estimator-form-input\">\r\n                <input class=\"form-input\" type=\"text\" id=\"shipping-city\" name=\"shipping-city\" value=\"{{selected_city}}\" placeholder=\"{{lang 'cart.shipping_estimator.suburb_city'}}\">\r\n            </dd>\r\n\r\n            <dt class=\"estimator-form-label\">\r\n                <label class=\"form-label\" for=\"shipping-zip\">{{lang 'cart.shipping_estimator.zip_postal_code'}}</label>\r\n            </dt>\r\n            <dd class=\"estimator-form-input\">\r\n                <input class=\"form-input\" type=\"text\" id=\"shipping-zip\" name=\"shipping-zip\" value=\"{{selected_zip}}\" placeholder=\"{{lang 'cart.shipping_estimator.zip_postal_code'}}\">\r\n            </dd>\r\n            <button class=\"button button--primary button--small shipping-estimate-submit mdc-button primary\">{{lang 'cart.shipping_estimator.estimate_shipping'}}</button>\r\n        </dl>\r\n    </form>\r\n    <div class=\"shipping-quotes\"></div>\r\n</div>\r\n","components/cart/totals":"<ul class=\"cart-totals\">\r\n    <li class=\"cart-total\">\r\n        <div class=\"cart-total-label mdc-typography-roman-regular-normal \">\r\n            <strong class=\"mdc-typography-roman-regular-normal \">Discounts</strong>\r\n        </div>\r\n        <div class=\"cart-total-value mdc-typography-roman-regular-normal \">\r\n            <span class=\"checkout-totals-discounts\">Apply at checkout</span>\r\n        </div>\r\n    </li>\r\n\r\n    <li class=\"cart-total first\">\r\n        <div class=\"cart-total-label mdc-typography-roman-regular-normal \">\r\n            <strong class=\"mdc-typography-roman-regular-normal \">{{lang 'cart.checkout.subtotal'}}</strong>\r\n        </div>\r\n        <div class=\"cart-total-value mdc-typography-roman-regular-normal \">\r\n            {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n            <span>{{cart.sub_total.formatted}}</span> {{else}} {{> components/common/login-for-pricing}} {{/or}}\r\n        </div>\r\n    </li>\r\n    {{#if cart.gift_wrapping_cost.amount}}\r\n    <li class=\"cart-total mdc-typography-roman-regular-normal \">\r\n        <div class=\"cart-total-label mdc-typography-roman-regular-normal \">\r\n            <strong>{{lang 'cart.checkout.gift_wrapping'}}</strong>\r\n        </div>\r\n        <div class=\"cart-total-value mdc-typography-roman-regular-normal \">\r\n            <span>{{cart.gift_wrapping_cost.formatted}}</span>\r\n        </div>\r\n    </li>\r\n    {{/if}} {{#if cart.shipping_handling.show_estimator}}\r\n    <li class=\"cart-total mdc-typography-roman-regular-normal  checkout-page-only\">\r\n        <div class=\"cart-total-label\" mdc-typography-roman-regular-normal>\r\n            <strong>{{lang 'cart.checkout.shipping'}}</strong>\r\n        </div>\r\n        {{> components/cart/shipping-estimator cart.shipping_handling}}\r\n    </li>\r\n    <!-- {{/if}} {{#each cart.taxes}}\r\n    <li class=\"cart-total mdc-typography-roman-regular-normal \">\r\n        <div class=\"cart-total-label mdc-typography-roman-regular-normal \">\r\n            <strong>{{name}}:</strong>\r\n        </div>\r\n        <div class=\"cart-total-value mdc-typography-roman-regular-normal \">\r\n            <span>{{cost.formatted}}</span>\r\n        </div>\r\n    </li>\r\n    {{/each}} -->\r\n\r\n    {{#if cart.coupons}}\r\n        {{#each cart.coupons}}\r\n            <li class=\"cart-total coupons-data-show-checkout\">\r\n                <div class=\"cart-total-label mdc-typography-roman-regular-normal \">\r\n                    <strong>{{lang 'cart.coupons.code_label' code=code}}</strong>\r\n                    <a class=\"coupons-remove-buttons\" url=\"{{remove_url}}\">{{lang 'cart.coupons.remove'}}</a>\r\n                </div>\r\n                <div class=\"cart-total-value mdc-typography-roman-regular-normal \">\r\n                    <span>{{discount.formatted}}</span>\r\n                </div>\r\n            </li>\r\n        {{/each}}\r\n    {{/if}}    \r\n    \r\n    <li class=\"cart-total mdc-typography-roman-regular-normal checkout-page-show\">\r\n        <div class=\"cart-total-label mdc-typography-roman-regular-normal \">\r\n            <strong>Total</strong>\r\n        </div>\r\n        <div class=\"cart-total-value cart-total-grandTotal mdc-typography-roman-regular-normal\">\r\n            {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n            <span>{{cart.grand_total.formatted}}</span> {{else}} {{> components/common/login-for-pricing}} {{/or}}\r\n        </div>\r\n    </li>\r\n</ul>","components/common/alert-error":"<div class=\"alertBox alertBox--error\">\r\n    <div class=\"alertBox-column alertBox-icon\">\r\n        <icon glyph=\"ic-error\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\r\n    </div>\r\n    <p class=\"alertBox-column alertBox-message\">\r\n        <span>{{{this}}}</span>\r\n    </p>\r\n</div>\r\n","components/common/alert-modal":"<div id=\"alert-modal\" class=\"modal modal--alert modal--small\" data-reveal data-prevent-quick-search-close>\r\n    <div class=\"swal2-icon swal2-error swal2-animate-error-icon\"><span class=\"swal2-x-mark swal2-animate-x-mark\"><span class=\"swal2-x-mark-line-left\"></span><span class=\"swal2-x-mark-line-right\"></span></span></div>\r\n\r\n    <div class=\"modal-content\"></div>\r\n\r\n    <div class=\"button-container\"><button type=\"button\" class=\"confirm button\" data-reveal-close>OK</button></div>\r\n</div>\r\n","components/common/alert-success":"<div class=\"alertBox alertBox--success\">\r\n    <div class=\"alertBox-column alertBox-icon\">\r\n        <icon glyph=\"ic-success\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\r\n    </div>\r\n    <p class=\"alertBox-column alertBox-message\">\r\n        <span>{{{this}}}</span>\r\n    </p>\r\n</div>\r\n","components/common/body":"<div class=\"body\" data-currency-code=\"{{currency_selector.active_currency_code}}\">\r\n    {{#block \"hero\"}} {{/block}}\r\n    <div class=\"container\">\r\n        {{#block \"page\"}} {{/block}}\r\n    </div>\r\n    {{> components/common/modal}}\r\n    {{> components/common/alert-modal}}\r\n</div>\r\n","components/common/breadcrumbs":"<ul class=\"breadcrumbs\" itemscope itemtype=\"http://schema.org/BreadcrumbList\">\r\n    {{#unless theme_settings.hide_breadcrumbs }}\r\n        {{#each breadcrumbs}}\r\n            <li class=\"breadcrumb {{#if @last}}is-active{{/if}}\" itemprop=\"itemListElement\" itemscope itemtype=\"http://schema.org/ListItem\">\r\n                {{#or @last (if url \"==\" null)}}\r\n                    <meta itemprop=\"item\" content=\"{{url}}\">\r\n                    <span class=\"breadcrumb-label\" itemprop=\"name\">{{name}}</span>\r\n                {{else}}\r\n                    <a href=\"{{url}}\" class=\"breadcrumb-label\" itemprop=\"item\"><span itemprop=\"name\">{{name}}</span></a>\r\n                {{/or}}\r\n                <meta itemprop=\"position\" content=\"{{add @index 1}}\" />\r\n            </li>\r\n        {{/each}}\r\n    {{/unless}}\r\n</ul>\r\n","components/common/cookie":"<div class=\"cookieMessage\">\r\n    <div class=\"cookieMessage-container\">\r\n        <div class=\"cookieMessage-text\">\r\n            <p>{{settings.privacy_cookie}}</p>\r\n        </div>\r\n        <button class=\"button button--primary button--small\" data-privacy-accept>{{lang 'cookies.accept'}}</button>\r\n    </div>\r\n</div>\r\n","components/common/footer":"{{#if template '==' 'pages/home'}}\r\n\r\n {{else}}\r\n<footer class=\"wc-footer-custom\">\r\n    <div class=\"wc-footer__menu-custom\">\r\n        <a class=\"wc-footer__menu-custom-link\" href=\"https://support.wildcrafter.com/hc/en-us/categories/360002006492-Returns\">Refund Policy</a>\r\n    </div>\r\n    <div class=\"wc-footer__menu-custom\">\r\n        <a class=\"wc-footer__menu-custom-link\" href=\"https://wildcrafter.com/privacy-policy/\">Privacy Policy</a>\r\n    </div>\r\n    <div class=\"wc-footer__menu-custom\">\r\n        <a class=\"wc-footer__menu-custom-link\" href=\"https://wildcrafter.com/terms-of-service/\">Terms of service</a>\r\n    </div>\r\n</footer>\r\n{{/if}}","components/common/forms/checkbox":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\r\n    <label class=\"form-label\" for=\"{{id}}\">{{label}}\r\n        {{#if required}}\r\n            <small>{{lang 'common.required' }}</small>\r\n        {{/if}}\r\n    </label>\r\n    {{#if checkboxes}}\r\n        {{#each checkboxes}}\r\n            <input type=\"checkbox\" value=\"{{value}}\" name=\"{{name}}\" id=\"{{name}}\" data-label=\"{{label}}\" class=\"form-checkbox\" {{#if checked}}checked{{/if}} {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n            <label class=\"form-label {{class}}\" for=\"{{name}}\">{{label}}</label>\r\n        {{/each}}\r\n    {{/if}}\r\n</div>\r\n","components/common/forms/date":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\r\n    <label class=\"form-label\" for=\"{{id}}\" >{{label}}\r\n    {{#if required}}<small>{{lang 'common.required' }}</small>{{/if}}</label>\r\n    <div class=\"form-row form-row--third\">\r\n        <div class=\"form-field\">\r\n            <select class=\"form-select\" name=\"{{month.name}}\" id=\"{{id}}_month\" data-label=\"month\"  aria-required=\"{{required}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n                {{#each month.items}}\r\n                    <option {{#if ../month.value '==' value}} selected {{/if}}value=\"{{value}}\">{{label}}</option>\r\n                {{/each}}\r\n            </select>\r\n        </div>\r\n        <div class=\"form-field\">\r\n            <select class=\"form-select\" name=\"{{day.name}}\" id=\"{{id}}_day\" data-label=\"day\" aria-required=\"{{required}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n                {{#each day.items}}\r\n                    <option {{#if ../day.value '==' value}} selected {{/if}}value=\"{{value}}\">{{label}}</option>\r\n                {{/each}}\r\n            </select>\r\n        </div>\r\n        <div class=\"form-field\">\r\n            <select class=\"form-select\" name=\"{{year.name}}\" id=\"{{id}}_year\" data-label=\"year\" aria-required=\"{{required}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n                {{#each year.items}}\r\n                    <option {{#if ../year.value '==' value}} selected {{/if}}value=\"{{value}}\">{{label}}</option>\r\n                {{/each}}\r\n            </select>\r\n        </div>\r\n    </div>\r\n</div>\r\n","components/common/forms/multiline":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\">\r\n    <label class=\"form-label\" for=\"{{id}}_input\">{{label}}\r\n        {{#if required}}\r\n            <small>{{lang 'common.required' }}</small>\r\n        {{/if}}\r\n    </label>\r\n    <textarea name=\"{{name}}\" id=\"{{id}}_input\" data-label=\"{{label}}\" rows=\"{{rows}}\" aria-required=\"{{required}}\" class=\"form-input\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>{{value}}</textarea>\r\n</div>\r\n","components/common/forms/number":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\">\r\n    <label class=\"form-label\" for=\"{{id}}_input\">{{label}}\r\n        {{#if required}}<small>{{lang 'common.required' }}</small>{{/if}}\r\n    </label>\r\n    <input type=\"number\" class=\"form-input\" data-label=\"{{label}}\" data-rule=\"lowerThan\" min=\"{{limitfrom}}\" max=\"{{limitto}}\" title=\"{{lang 'forms.range' limitFrom=limitfrom limitTo=limitto}}\" value=\"{{value}}\" id=\"{{id}}_input\" name=\"{{name}}\" aria-required=\"{{required}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n</div>\r\n","components/common/forms/password":"<div class=\"form-field\">\r\n<div class=\"mdc-ripple-upgraded mdc-text-field mdc-text-field--with-trailing-icon\"  id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\r\n    <i class=\"material-icons mdc-text-field__icon icon-align-left\">https</i>\r\n    <label class=\"form-label mdc-floating-label label-align-left\" for=\"{{id}}_input\">{{label}}      \r\n    </label>\r\n    <input type=\"password\"  class=\"form-input mdc-text-field__input\" id=\"{{id}}_input\" data-label=\"{{label}}\" name=\"{{name}}\" value=\"{{value}}\" autocomplete=\"off\" \r\n    aria-required=\"{{required}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n</div>\r\n</div>\r\n\r\n<!-- \r\n<div class=\"mdc-ripple-upgraded mdc-text-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\r\n    <label class=\"mdc-floating-label\" for=\"{{id}}_input\">{{label}}        \r\n    </label>\r\n    <input type=\"password\" id=\"{{id}}_input\" data-label=\"{{label}}\" name=\"{{name}}\" {{#if value}} value=\"{{value}}\"{{/if}} autocomplete=\"off\" \r\n    class=\"mdc-text-field__input\" aria-required=\"{{required}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n    {{#if required}}<div class=\"mdc-line-ripple\"></div>{{/if}}\r\n</div> -->","components/common/forms/radio":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\">\r\n    <label class=\"form-label\">{{label}}\r\n        {{#if required}}\r\n            <small>{{lang 'common.required' }}</small>\r\n        {{/if}}\r\n    </label>\r\n    {{#if radios}}\r\n        {{#each radios}}\r\n            <input \r\n                type=\"radio\"\r\n                class=\"form-radio\"\r\n                name=\"{{../name}}\"\r\n                id=\"{{../name}}_{{@index}}\"\r\n                valu