iv>\r\n        <div class=\"wc-header__logo-wrapper\">\r\n            <a class=\"wc-header__logo-container\" href=\"../\">\r\n                <!-- <img class=\"wc-header__logo-large\" src=\"{{cdn 'tiles/wildcrafter-logo.svg'}}\"></img> -->\r\n                <img class=\"wc-header__logo-large\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\"></img>\r\n                <i class=\"wc-header__logo-sticky icon-wildcrafter-logo\"></i>\r\n                \r\n            </a>\r\n            {{#if template '==' 'pages/home'}}\r\n            <style>\r\n                .wc-header__logo-wrapper{top:220%}\r\n                .loading {font-size: 20px;text-align: left;margin-left: 20px; margin-top:10px;cursor: pointer}                  \r\n                .loading:after {overflow: hidden;display: inline-block;vertical-align: bottom;-webkit-animation: ellipsis steps(4,end) 900ms infinite; animation: ellipsis steps(4,end) 900ms infinite;content: \"\\2026\"; width: 0px; } \r\n                                  \r\n                @keyframes ellipsis {\r\n                  to {\r\n                    width: 1.25em;    \r\n                  }\r\n                }\r\n\r\n                @-webkit-keyframes ellipsis {\r\n                  to {\r\n                    width: 1.25em;    \r\n                  }\r\n                }\r\n            </style>\r\n           <div class=\"loading\" onclick=\"window.location.href='http://wildcrafter.com/'\">Redirecting to Shop</div>\r\n            {{/if}}\r\n        </div>\r\n    </div>\r\n</header>\r\n\r\n<div class=\"wc-header-placeholder\"></div>\r\n","components/common/login-for-pricing":"<p translate>Log in for pricing</p>","components/common/modal":"<div id=\"modal\" class=\"modal\" data-reveal data-prevent-quick-search-close>\r\n    <a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" role=\"button\">\r\n        <span aria-hidden=\"true\">&#215;</span>\r\n    </a>\r\n    <div class=\"modal-content\"></div>\r\n    <div class=\"loadingOverlay\"></div>\r\n</div>\r\n","layout/base":"<!DOCTYPE html>\r\n<html class=\"no-js\" lang=\"{{ locale_name }}\">\r\n    <head>\r\n        <title>{{ head.title }}</title>\r\n        {{{ resourceHints }}}\r\n        {{{ head.meta_tags }}}\r\n        {{{ head.config }}}\r\n        {{#block \"head\"}} {{/block}}\r\n\r\n        <link href=\"{{ head.favicon }}\" rel=\"shortcut icon\">\r\n        <link rel=\"stylesheet\" href=\"https://fonts.googleapis.com/icon?family=Material+Icons\">\r\n        <meta name=\"viewport\" content=\"width=device-width, initial-scale=1, maximum-scale=1\">\r\n        <!-- <meta http-equiv=\"refresh\" content=\"0;URL=/test.php\"> -->\r\n        <script src=\"{{cdn 'assets/js/custom/jquery-ver=1.12.4.js'}}\"></script>\r\n\r\n        <script>\r\n            {{!-- Change document class from no-js to js so we can detect this in css --}}\r\n            document.documentElement.className = document.documentElement.className.replace('no-js', 'js');\r\n        </script>\r\n        \r\n        <script>\r\n        window.addEventListener('load', function() {\r\n            if (top == self) {\r\n                document.querySelector('.wc-header').style.display = 'block';\r\n                document.querySelector('.wc-footer-custom').style.display = 'block';\r\n                jQuery(\"div\").removeClass('framework');\r\n                if(jQuery(\"div\").hasClass(\"fixed_area\"))\r\n                    document.querySelector('.fixed_area').style.position = 'relative';\r\n                //jQuery(\".container\").css(\"padding\", \"0px\");\r\n            }\r\n        })\r\n\r\n        </script>\r\n\r\n        {{ getFontsCollection }}\r\n        {{{stylesheet '/assets/css/theme.css'}}}\r\n\r\n        {{{head.scripts}}}\r\n        {{{head.rsslinks}}}\r\n\r\n        {{~inject 'themeSettings' theme_settings}}\r\n        {{~inject 'genericError' (lang 'common.generic_error')}}\r\n        {{~inject 'maintenanceMode' settings.maintenance}}\r\n        {{~inject 'urls' urls}}\r\n        {{~inject 'secureBaseUrl' settings.secure_base_url}}\r\n        {{~inject 'cartId' cart_id}}\r\n        {{~inject 'template' template}}\r\n    </head>\r\n    <body>\r\n        <svg data-src=\"{{cdn 'img/icon-sprite.svg'}}\" class=\"icons-svg-sprite\"></svg>\r\n\r\n        {{~#and settings.privacy_cookie settings.is_eu_ip_address}}\r\n            {{> components/common/cookie}}\r\n        {{/and}}\r\n\r\n        {{> components/common/header }}\r\n        {{> components/common/body }}\r\n        {{> components/common/footer }}\r\n\r\n        <script>window.__webpack_public_path__ = \"{{cdn 'assets/dist/'}}\";</script>\r\n        <script src=\"{{cdn 'assets/dist/theme-bundle.main.js'}}\"></script>\r\n        <script>\r\n            {{!-- Exported in app.js --}}\r\n            window.stencilBootstrap(\"{{page_type}}\", {{jsContext}}).load();\r\n        </script>\r\n\r\n        {{{footer.scripts}}}\r\n        <script src=\"{{cdn 'assets/js/custom/main.min.js'}}\"></script>\r\n        <script src=\"{{cdn 'assets/js/custom/vendor.min.js'}}\"></script>\r\n    </body>\r\n</html>\r\n","pages/account/edit":"{{inject 'passwordRequirements' settings.password_requirements}}\r\n{{#partial \"page\"}}\r\n<div class=\"account account--fixed\">\r\n    {{> components/common/breadcrumbs breadcrumbs=breadcrumbs}}\r\n    <h2 class=\"page-heading\">{{lang 'account.settings.heading' }}</h2>\r\n    {{> components/account/navigation account_page='settings'}}\r\n    {{#if forms.edit_account.error}}\r\n    {{> components/common/alert-error forms.edit_account.error}}\r\n    {{/if}}\r\n    {{#if forms.edit_account.success}}\r\n    {{> components/common/alert-success forms.edit_account.success}}\r\n    {{/if}}\r\n    {{> components/account/edit-account}}\r\n</div>\r\n{{/partial}}\r\n{{> layout/base}}\r\n"}
